{
    "name": "medicall",
    "version": "1.0.0",
    "description": "capstone exp",
    "scripts": {
        "build": "npm install  && cd APIs && npm install  && cd ../frontend && npm install && npm run build",
        "server": "cd APIs && node server.js",
        "client": "npm start --prefix frontend",
        "start": "concurrently \"npm run server\" \"npm run client\""
    },
    "devDependencies": {
        "concurrently": "^7.2.0"
    },
    "engines": {
        "npm": ">=6.0.0",
        "node": ">=16.0.0"
    }
}